-- Load a table literal
X = TABLE [(1,"Fred", 32000), (2, "Joe", 13000), (3, "Sally", 12332)] AS
  (id:int, name:string, salary:int);

-- Dump it to standard out
DUMP X;

/* load another table */
Y = TABLE [(4,"Henry", 221342), (5, "Heather", 32423), (6, "Chuck", 7032)] AS
  (id:int, name:string, salary:int);

/* assignment */
Z = Y;
DUMP Z;

Z = UNION X, Y;
A = INTERSECT X, Y;
